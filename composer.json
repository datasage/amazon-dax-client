{
    "name": "datasage/amazon-dax-client",
    "description": "Amazon DynamoDB Accelerator (DAX) Client for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": ["aws", "amazon", "dynamodb", "dax", "cache"],
    "authors": [
        {
            "name": "Dan Johansson",
            "email": "datasage@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-sockets": "*",
        "ext-openssl": "*",
        "aws/aws-sdk-php": "^3.0",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "spomky-labs/cbor-php": "^3.0"
    },
    "require-dev": {
        "monolog/monolog": "^3.0",
        "php-cs-fixer/shim": "^3.85",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dax\\": "src/Dax/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dax\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
